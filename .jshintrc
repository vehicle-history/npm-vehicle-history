{
  "esversion":  6,
  "es3":        false,
  "es5":        false,
  "bitwise":    false,
  "asi":        false,
  "browser":    false,
  "camelcase":  true,
  "curly":      true,
  "devel":      false,
  "enforceall": true,
  "eqeqeq":     true,
  "eqnull":     true,
  "forin":      true,
  "funcscope":  false,
  "immed":      true,
  "indent":     2,
  "laxbreak":   false,
  "laxcomma":   false,
  "loopfunc":   true,
  "newcap":     true,
  "noarg":      true,
  "nocomma":    false,
  "noempty":    true,
  "nonew":      true,
  "notypeof":   true,
  "node":       true,
  "quotmark":   "single",
  "plusplus":   false,
  "shadow":     true,
  "strict":     false,
  "sub":        false,
  "supernew":   true,
  "trailing":   true,
  "undef":      true,
  "unused":     true,
  "maxlen":     140,
  "latedef":    true,
  "globals" : {
    "JSON": true,
    "require": true
  }
}
